// Automaton
digraph {
	init_S0 [label="" height=0.1 shape=point width=0.1]
	S0 [label=S0 shape=circle]
	init_S0 -> S0 [color=green]
	S0 -> S1 [label=a]
	S0 -> S0 [label=b]
	S0 -> S2 [label=c]
	S0 -> S3 [label=d]
	S1 [label=S1 shape=doublecircle]
	S2 [label=S2 shape=circle]
	S2 -> S0 [label=c]
	S3 [label=S3 shape=doublecircle]
	S3 -> S1 [label=a]
	S3 -> S0 [label=b]
	S3 -> S2 [label=c]
	S3 -> S3 [label=d]
	rankdir=LR size="16,10"
	node [shape=circle]
	dpi=300
}

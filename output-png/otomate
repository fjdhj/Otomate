// Automaton
digraph {
	init_q1 [label="" height=0.1 shape=point width=0.1]
	q1 [label=q1 shape=circle]
	init_q1 -> q1 [color=green]
	q1 -> q1 [label=a1]
	q1 -> q2 [label=b]
	q2 [label=q2 shape=doublecircle]
	q2 -> q1 [label=b]
	q2 -> q2 [label=a2]
	rankdir=LR size="16,10"
	node [shape=circle]
	dpi=300
}
